{
    "name": "One Tap Boost",
    "description": "One Tap Boost",
    "version": "1.3.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "permissions": ["storage", "activeTab", "scripting", "cookies", "proxy", "webRequestAuthProvider", "webRequest", "management"],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "/images/OneTapBoost.png",
            "32": "/images/OneTapBoost.png",
            "48": "/images/OneTapBoost.png",
            "128": "/images/OneTapBoost.png"
        }
    },
    "icons": {
        "16": "/images/OneTapBoost.png",
        "32": "/images/OneTapBoost.png",
        "48": "/images/OneTapBoost.png",
        "128": "/images/OneTapBoost.png"
    },
    "content_scripts": [{
        "matches": ["https://*.youtube.com/*", "https://*/*", "http://*/*"],
        "js": ["content.js"]
    }]
}