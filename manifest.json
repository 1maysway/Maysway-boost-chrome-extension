{
    "name": "MW Boost",
    "description": "YouTube boost",
    "version": "1.2.2",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "permissions": ["storage", "activeTab", "scripting", "cookies"],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "/images/logo.jpg",
            "32": "/images/logo.jpg",
            "48": "/images/logo.jpg",
            "128": "/images/logo.jpg"
        }
    },
    "icons": {
        "16": "/images/logo.jpg",
        "32": "/images/logo.jpg",
        "48": "/images/logo.jpg",
        "128": "/images/logo.jpg"
    },
    "content_scripts": [{
        "matches": ["https://*.youtube.com/*", "https://*/*", "http://*/*"],
        "js": ["content.js"]
    }]
}